{
  "name": "riebot-v5",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf ./build",
    "build": "tsc",
    "postbuild": "rsync -av ./lib/secrets ./lib/web ./build --exclude web/scripts",
    "prestart": "npm run build",
    "start": "node --no-experimental-fetch -r source-map-support/register -r dotenv/config ./build/index.js dotenv_config_path=./build/secrets/.env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Likely-Tim/Riebot-v5.git"
  },
  "author": "Timothy Wu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Likely-Tim/Riebot-v5/issues"
  },
  "homepage": "https://github.com/Likely-Tim/Riebot-v5#readme",
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-openid-connect": "^2.11.0",
    "keyv": "^4.5.2",
    "keyv-file": "^0.2.0",
    "node-fetch": "^2.6.7",
    "node-html-markdown": "^1.3.0",
    "typescript": "^4.9.4",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.12",
    "@types/node-fetch": "^2.6.2",
    "@types/spotify-api": "^0.0.19",
    "prettier": "^2.8.1",
    "source-map-support": "^0.5.21"
  }
}
